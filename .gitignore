# Ignore bundler config.
/.bundle

# Ignore vendor/cache
/vendor/cache
/vendor/bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep
rails.log

/node_modules
/yarn-error.log

.byebug_history

# OS generated files #
# ######################
.DS_Store
.DS_Store?
._*

# Ignore local environmental variables
/config/local_env.yml

*.tar